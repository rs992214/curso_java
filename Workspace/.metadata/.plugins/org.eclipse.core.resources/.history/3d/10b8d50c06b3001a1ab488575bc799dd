package com.ejemplor.relaciones.dto;

public class PersonaSalidaDto {

	private int id;
	private String nombre;
	private String apellido;
	private int tipo_documento_id;
	private TipoDocumentoDto documento;
	private String numerodocumento;
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public String getApellido() {
		return apellido;
	}
	public void setApellido(String apellido) {
		this.apellido = apellido;
	}
	public int getTipo_documento_id() {
		return tipo_documento_id;
	}
	public void setTipo_documento_id(int tipo_documento_id) {
		this.tipo_documento_id = tipo_documento_id;
	}
	public TipoDocumentoDto getDocumento() {
		return documento;
	}
	public void setDocumento(TipoDocumentoDto documento) {
		this.documento = documento;
	}
	public String getNumerodocumento() {
		return numerodocumento;
	}
	public void setNumerodocumento(String numerodocumento) {
		this.numerodocumento = numerodocumento;
	}
	
}
