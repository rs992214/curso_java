package cartas.mazo;

import java.util.*;

public class Mazo {
	ArrayList<Carta> cartas;

	public Mazo() {
		super();
		String palos[] = {"espada","basto","copa","oro"};
		int numeros[] ={1,2,3,4,5,6,7,10,11,12};
		for(int i = 0; i<4;i++){
			for(int j = 0; j>10; j++){
				cartas.add(new Carta(numeros[j]),palos[i]);
			}
			
		}
	}
	
	

}
